{
  "name": "zzapi",
  "displayName": "zzAPI",
  "publisher": "AgroStar",
  "description": "Tool for testing and documenting REST APIs",
  "repository": "https://github.com/agrostar/zzapi-vscode",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "api",
    "rest",
    "http",
    "client",
    "postman",
    "testing",
    "api documentation"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "dist/extension.js",
  "icon": "images/zzapi.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.clickEnvSelector",
        "title": "zzAPI: Select Environment"
      },
      {
        "command": "extension.importPostman",
        "title": "zzAPI: Import Postman Collection"
      },
      {
        "command": "extension.importEnvironment",
        "title": "zzAPI: Import Postman Environment"
      },
      {
        "command": "extension.showVariables",
        "title": "zzAPI: Show Variables"
      },
      {
        "command": "extension.showRecentHeaders",
        "title": "zzAPI: Show Recent Reponse Headers"
      },
      {
        "command": "extension.showSampleGET",
        "title": "zzAPI: Add Sample GET Request"
      },
      {
        "command": "extension.showSamplePOST",
        "title": "zzAPI: Add Sample POST Request"
      }
    ],
    "submenus": [
      {
        "id": "zzapi_submenu",
        "label": "zzAPI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "zzapi_submenu",
          "group": "zzAPI"
        }
      ],
      "zzapi_submenu": [
        {
          "command": "extension.importPostman"
        },
        {
          "command": "extension.importEnvironment"
        },
        {
          "command": "extension.showVariables"
        },
        {
          "command": "extension.showRecentHeaders"
        },
        {
          "command": "extension.showSampleGET",
          "when": "resourceExtname == .zzb"
        },
        {
          "command": "extension.showSamplePOST",
          "when": "resourceExtname == .zzb"
        }
      ]
    },
    "configuration": {},
    "yamlValidation": [
      {
        "fileMatch": [
          "*.zzb"
        ],
        "url": "https://raw.githubusercontent.com/agrostar/zzapi/main/zzapi-bundle.schema.json"
      },
      {
        "fileMatch": [
          "*.zzv"
        ],
        "url": "https://raw.githubusercontent.com/agrostar/zzapi/main/zzapi-varset.schema.json"
      }
    ],
    "languages": [
      {
        "id": "yaml",
        "extensions": [
          ".zzb",
          ".zzv",
          ".zzx"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "3.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "got": "11.8.6",
    "jsonpath": "^1.1.1",
    "yaml": "^2.3.1"
  }
}
