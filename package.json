{
  "name": "zzapi",
  "displayName": "zzAPI",
  "publisher": "AgroStar",
  "description": "Tool for testing and documenting REST APIs",
  "repository": "https://github.com/agrostar/zzapi-vscode",
  "version": "0.10.2",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "api",
    "rest",
    "http",
    "client",
    "postman",
    "testing",
    "api documentation"
  ],
  "activationEvents": [
    "workspaceContains:**/*.zzb"
  ],
  "main": "out/extension.js",
  "icon": "images/zzapi.jpg",
  "contributes": {
    "commands": [
      {
        "command": "extension.clickEnvSelector",
        "title": "zzAPI: Select Environment"
      },
      {
        "command": "extension.importPostman",
        "title": "zzAPI: Import Postman collection"
      },
      {
        "command": "extension.showVariables",
        "title": "zzAPI: Show Variables"
      },
      {
        "command": "extension.showRecentHeaders",
        "title": "zzAPI: Show Recent Reponse Headers"
      }
    ],
    "submenus": [
      {
        "id": "zzapi_submenu",
        "label": "zzAPI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "zzapi_submenu",
          "group": "zzAPI"
        }
      ],
      "zzapi_submenu": [
        {
          "command": "extension.importPostman"
        },
        {
          "command": "extension.showVariables"
        },
        {
          "command": "extension.showRecentHeaders"
        }
      ]
    },
    "configuration": {},
    "yamlValidation": [
      {
        "fileMatch": [
          "*.zzb"
        ],
        "url": "https://agrostar.in/schemas/zzapi-bundle.schema.json"
      },
      {
        "fileMatch": [
          "*.zzv"
        ],
        "url": "https://raw.githubusercontent.com/agrostar/zzapi/main/zzapi-varset.schema.json"
      }
    ],
    "languages": [
      {
        "id": "yaml",
        "extensions": [
          ".zzb",
          ".zzv",
          ".zzx"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "3.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "got": "11.8.6",
    "jsonpath": "^1.1.1",
    "yaml": "^2.3.1"
  }
}
